global:
  slack_api_url: '<%= @slack_webhook %>'

receivers:

- name: 'slack'
  slack_configs:
    - channel: '{{ .CommonLabels.channel }}'
      username: 'AlertManager'
      send_resolved: false
      title: '{{ .CommonLabels.alertname }}'
      text: "{{ range .Alerts }}{{ .Annotations.description }}\n{{ end }}"
